<api xmlns="http://ws.apache.org/ns/synapse" name="payment:vv1.0.0" context="/payment/v1.0.0">
    <resource methods="DELETE POST PUT GET" url-mapping="/*">
        <inSequence>
            <sequence key="paymentMainSeq"/>
        </inSequence>
        <outSequence>
            <sequence key="paymentOutMainSeq"/>
            <respond/>
        </outSequence>
        <faultSequence/>
    </resource>
</api>
